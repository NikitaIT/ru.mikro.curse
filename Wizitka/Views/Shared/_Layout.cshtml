                        <div id="fh5co-logo">@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })<a href=""><img style="width: 72px" src="/Content/images/logo.jpg" alt="Картинка"></a></div>
                        <nav id="fh5co-main-nav" role="navigation">
                            <ul>
                                <li>@Html.ActionLink("About", "About", "Home")</li>
